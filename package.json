{
  "name": "static-web-experiments",
  "version": "0.1.0",
  "private": "true",
  "description": "An analytics module using TypeScript",
  "source": [
    "src/index.html"
  ],
  "browserslist": "> 0.5%, last 10 versions, not dead",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build:prod": "parcel build --public-url $PUBLIC_URL",
    "check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "keywords": [
    "analytics",
    "typescript"
  ],
  "author": "T. Siva",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.8.3",
    "prettier": "2.8.6",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">= 14"
  }
}
